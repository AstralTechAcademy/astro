const buttonStyle = "text-black bg-blue-400 hover:bg-blue-500 focus:ring-4 focus:outline-none focus:ring-blue-300 font-normal rounded-lg text-m w-full sm:w-auto px-5 py-2.5 text-center"

---

<Layout >
  	<div class="flex items-center justify-center relative top-15">
		<button 
			onclick="window.location.href='/register'" 
			class={buttonStyle}>Registrarse</button>
	</div>
</Layout>
